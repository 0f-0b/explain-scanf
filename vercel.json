{
  "functions": {
    "api/**/*.ts": {
      "runtime": "vercel-deno@0.7.9"
    }
  },
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "destination": "/api/code",
      "source": "/api/code/:id"
    },
    {
      "destination": "/",
      "source": "/((?!@|src/|node_modules/).*)"
    }
  ]
}
